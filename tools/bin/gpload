#!/bin/sh
if [ ! -z "$GPHOME" ]; then
    . $GPHOME/greenplum_path.sh
fi
if [ ! -z "$GPHOME_LOADERS" ]; then
    . $GPHOME_LOADERS/greenplum_loaders_path.sh
fi
if [ `uname -s` = "AIX" -a ! -z "$GP_LIBPATH_FOR_PYTHON" ]; then
    LIBPATH=$GP_LIBPATH_FOR_PYTHON:$LIBPATH
    export LIBPATH
fi


exec gpload.py $*
