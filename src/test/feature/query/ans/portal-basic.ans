--
-- NO SCROLL disallows backward fetching
--
BEGIN;
BEGIN
DECLARE foo24 NO SCROLL CURSOR FOR SELECT * FROM test1 ORDER BY 1,2,3,4;
DECLARE CURSOR
FETCH 1 FROM foo24;
 a | b | c | d 
---+---+---+---
 1 | 2 | 3 | 4
(1 row)

FETCH BACKWARD 1 FROM foo24; -- should fail
psql:/tmp/TestQueryPortal_TestBasic1.sql:143: ERROR:  backward scan is not supported in this version of Greenplum Database
END;
ROLLBACK
